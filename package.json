{
  "name": "blog.ltgt.net",
  "private": true,
  "scripts": {
    "build:eleventy": "npx @11ty/eleventy",
    "build:playground-elements": "npx rollup -p @rollup/plugin-node-resolve -p rollup-plugin-minify-html-template-literals -p @rollup/plugin-terser -f es -o js/playground-elements.js node_modules/playground-elements/index.js",
    "build:playground-typescript-worker": "mkdir -p js/ && cp node_modules/playground-elements/playground-typescript-worker.js js/",
    "build": "npm run build:playground-elements && npm run build:playground-typescript-worker && npm run build:eleventy",
    "serve": "npx @11ty/eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@jongwooo/prism-theme-github": "^1.7.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "markdown-it-anchor": "^8.6.7",
    "rollup": "^2.79.1",
    "rollup-plugin-minify-html-template-literals": "^1.2.0"
  },
  "dependencies": {
    "playground-elements": "^0.17.0"
  }
}
